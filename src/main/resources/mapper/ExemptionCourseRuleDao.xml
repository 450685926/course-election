            </if>
            <if test="courseName != null">
                COURSE_NAME,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">

            <if test="calendarId != null">
                #{calendarId,jdbcType=BIGINT},
            </if>
            <if test="trainingLevel != null">
                #{trainingLevel,jdbcType=VARCHAR},
            </if>
            <if test="formLearning != null">
