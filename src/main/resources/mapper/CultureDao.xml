<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.server.edu.election.dao.CultureDao">
    <select id="getStudentScoreByStudentId" parameterType="java.lang.String" resultType="java.lang.String">
        select COURSE_CODE_ FROM  exemption_score_t where STUDENT_CODE_ = #{studentId,jdbcType=VARCHAR}
    </select>

</mapper>