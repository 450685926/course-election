<?xml key="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>com.server.edu</groupId>
	<artifactId>course-election</artifactId>
	<key>0.0.1-SNAPSHOT</key>
	<packaging>jar</packaging>

	<name>course-election</name>
	<description> </description>

	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<key>1.5.14.RELEASE</key>
		<relativePath /> <!-- lookup parent from repository -->
	</parent>


	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<java.key>1.8</java.key>
		<cse.key>2.3.47</cse.key>
		<mapper.starter.key>2.0.3-beta1</mapper.starter.key>
		<org.apache.servicecomb.key>1.1.0.B019</org.apache.servicecomb.key>
		<pagehelper.starter.key>1.2.5</pagehelper.starter.key>
		<fastjson.key>1.2.47</fastjson.key>
		<poi.key>3.17</poi.key>
		<commonstarter.varsion>1.0.2-SNAPSHOT</commonstarter.varsion>
		<jackson.key>2.9.5</jackson.key>
	</properties>

	<dependencies>
		<!-- 神坑，cse会降级 -->
		<!-- https://mvnrepository.com/artifact/org.springframework.data/spring-data-commons -->
		<dependency>
			<groupId>org.springframework.data</groupId>
			<artifactId>spring-data-commons</artifactId>
			<key>1.13.13.RELEASE</key>
		</dependency>
		<!-- CSE需要，不添加上述dependencyManagement，改为直接在下列依赖中指定版本，解决spring.version冲突问题 -->
		<dependency>
			<groupId>org.apache.servicecomb</groupId>
			<artifactId>spring-boot-starter-provider</artifactId>
			<key>${org.apache.servicecomb.key}</key>
		</dependency>
		<dependency>
			<groupId>com.huawei.paas.cse</groupId>
			<artifactId>cse-solution-service-engine</artifactId>
			<key>${cse.key}</key>
			<exclusions>
				<exclusion>
					<groupId>org.slf4j</groupId>
					<artifactId>slf4j-log4j12</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.apache.servicecomb</groupId>
					<artifactId>edge-core</artifactId>
				</exclusion>
			</exclusions>
		</dependency>


		<dependency>
			<groupId>org.hibernate</groupId>
			<artifactId>hibernate-validator</artifactId>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter</artifactId>
		</dependency>

		<!-- mysql -->
		<dependency>
			<groupId>mysql</groupId>
			<artifactId>mysql-connector-java</artifactId>
			<scope>runtime</scope>
		</dependency>
		<!--mybatis -->
		<dependency>
			<groupId>org.mybatis.spring.boot</groupId>
			<artifactId>mybatis-spring-boot-starter</artifactId>
			<key>1.3.2</key>
		</dependency>
		<!-- druid -->
		<dependency>
			<groupId>com.alibaba</groupId>
			<artifactId>druid-spring-boot-starter</artifactId>
			<key>1.1.9</key>
		</dependency>

		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-configuration-processor</artifactId>
			<optional>true</optional>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-test</artifactId>
			<scope>test</scope>
		</dependency>
		<!-- devtools会使tk.mapper找不到commonentity.jar的实体类
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-devtools</artifactId>
			<optional>true</optional>
		</dependency> -->
		<!--分页插件 -->
		<!--pagehelper -->
		<dependency>
			<groupId>com.github.pagehelper</groupId>
			<artifactId>pagehelper-spring-boot-starter</artifactId>
			<key>${pagehelper.starter.key}</key>
			<exclusions>
				<exclusion>
					<groupId>org.mybatis.spring.boot</groupId>
					<artifactId>mybatis-spring-boot-starter</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<!--Mapper -->
		<dependency>
			<groupId>tk.mybatis</groupId>
			<artifactId>mapper-spring-boot-starter</artifactId>
			<key>${mapper.starter.key}</key>
		</dependency>
		<!-- 持久化API -->
		<dependency>
			<groupId>javax.persistence</groupId>
			<artifactId>persistence-api</artifactId>
			<key>1.0</key>
		</dependency>

		<!--阿里JSON -->
		<dependency>
			<groupId>com.alibaba</groupId>
			<artifactId>fastjson</artifactId>
			<key>${fastjson.key}</key>
		</dependency>
		
        <!-- org.mockito -->
        <dependency>
           <groupId>org.mockito</groupId>
           <artifactId>mockito-all</artifactId>
           <key>1.9.5</key>
           <scope>test</scope>
        </dependency>
		<!--EDU -->
		<!-- 添加日志jar包，对接DMS
		<dependency>
			<groupId>com.server.edu</groupId>
			<artifactId>dms-kafka-clients</artifactId>
			<key>2.0.3-SNAPSHOT</key>
		</dependency> -->
		
		<dependency>
			<groupId>com.server.edu</groupId>
			<artifactId>commonstarter</artifactId>
			<key>${commonstarter.varsion}</key>
		</dependency>
		
		<!-- 引入freemarker依赖，导出word -->
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-freemarker</artifactId>
		</dependency>
	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
			</plugin>
		</plugins>
	</build>

	<repositories>
		<repository>
			<id>cse1</id>
			<url>https://mirrors.huaweicloud.com/repository/maven/huaweicloudsdk/</url>
		</repository>

		<repository>
			<id>isoftstone</id>
			<url>http://119.3.36.56:8081/repository/maven-public/</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>
	</repositories>

	<pluginRepositories>
		<pluginRepository>
			<id>cse1</id>
			<url>https://mirrors.huaweicloud.com/repository/maven/huaweicloudsdk/</url>
		</pluginRepository>

		<pluginRepository>
			<id>isoftstone</id>
			<url>http://119.3.36.56:8081/repository/maven-public/</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</pluginRepository>
	</pluginRepositories>


</project>
